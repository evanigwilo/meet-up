    "dev-ts": "NODE_ENV=development nodemon --config configs/nodemon-ts.json",
    "dev-js": "tsc -w & (NODE_ENV=development DEV_LANG=js nodemon --config configs/nodemon-js.json -L)",
    "dev-secure": "PROTOCOL=https yarn dev-ts",
    "build": "tsc --project tsconfig.build.json",
    "start": "node build/api/v1/index.js",
    "production": "yarn build && NODE_ENV=production yarn start",
    "typeorm": "typeorm-ts-node-commonjs",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "NODE_ENV=testing jest --config jest.config.js --runInBand",
    "lint": "tslint -p tsconfig.json"
